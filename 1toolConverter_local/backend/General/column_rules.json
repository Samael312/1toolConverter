{
  "register": {
    "type": ["int", "hex", "float"],
    "pattern": ["^0x[0-9A-Fa-f]+$", "^[0-9]+$"]
  },
  "name": {
    "type": ["str"],
    "pattern": ["^[A-Z0-9_]+$"]
  },
  "description": {
    "type": ["str"],
    "min_length": 10
  },
  "unit": {
    "type": ["str"],
    "max_length": 5
  }
}
